{
  "name": "employeemanagementfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "swagger:fetch-gen": "curl -k -o src/generatedCode/swagger.json https://localhost:7088/swagger/v1/swagger.json && npm run swagger:gen",
    "swagger:gen": "npm run swagger:nswag",
    "swagger:nswag": "nswag run src/generatedCode/nswag.json"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nswag": "^14.0.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
