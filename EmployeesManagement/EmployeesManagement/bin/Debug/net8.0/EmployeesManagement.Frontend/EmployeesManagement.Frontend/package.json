
{
  "name": "employeemanagementfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "build-storybook": "deno run -A --max-old-space-size=8192 https://deno.land/x/storybook_build/mod.ts",
    "dev": "vite",
    "lint": "tsc --noEmit && deno lint 'src/**/*.{ts,tsx}' --unstable --fix",
    "lint:dryRun": "deno lint src/**/*.ts --unstable --dry-run",
    "lint:fix": "deno lint src/**/*.ts --unstable --fix",
    "lint:prettier": "deno fmt 'src/**/*' --options-prettier.json",
    "nibble": "deno lint 'src/**/*.{ts,tsx}' --unstable --ignore=*.cache",
    "nibble:fixable": "deno lint 'src/**/*.{ts,tsx}' --unstable --ignore=*.cache --fix",
    "prepare": "deno run -A --unstable https://deno.land/x/husky/mod.ts install",
    "sass:all": "deno run -A https://deno.land/x/sass/mod.ts",
    "sass:storybook": "deno run -A https://deno.land/x/sass/mod.ts src/css:./public/css --style=compressed",
    "sassIcons": "deno run -A https://deno.land/x/sass/mod.ts src/css/customization/themes/standard/app-icons.scss -o wwwroot/css/production/customization/themes/standard  --output-style compressed",
    "start": "vite",
    "start:productionConfig": "vite build --mode production",
    "storybook": "deno run -A https://deno.land/x/storybook/dev.ts -p 6006",
    "swagger:fetch-gen": "curl -k -o src/generatedCode/swagger.json https://localhost:7088/swagger/v1/swagger.json && deno run -A https://deno.land/x/nswag/mod.ts src/generatedCode/nswag.json",
    "swagger:gen": "deno run -A https://deno.land/x/nswag/mod.ts src/generatedCode/nswag.json",
    "test": "deno test --coverage",
    "test:ci": "deno test --coverage --reporter=junit",
    "test:coverage": "deno test --coverage",
    "test:ui": "deno test --watch",
    "tsc": "tsc"
  },
  "dependencies": {
    "bootstrap": "^5.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "^18.2.0",
    "react-icons": "^5.2.1",
    "react-router-bootstrap": "^0.26.2",
    "react-router-dom": "^6.23.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "nswag": "^14.0.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  }
}
