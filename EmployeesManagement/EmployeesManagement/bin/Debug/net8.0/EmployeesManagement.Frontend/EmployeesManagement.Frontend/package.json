{
  "name": "studentplanfrontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "swagger:fetch-gen": "curl -k -o src/generatedCode/swagger.json https://localhost:7088/swagger/v1/swagger.json && npm run swagger:gen && npm run gulp:copy:swagger",
    "swagger:gen": "npm run swagger:nswag && npm run swagger:patch && npm run swagger:fmt",
    "swagger:nswag": "nswag run src/generatedCode/nswag.json",
    "swagger:patch": "python tools/gen-swagger-query.py src/generatedCode/pbd-core-api.ts src/generatedCode/swagger.json",
    "swagger:fmt": "deno fmt src/generatedCode/pbd-core-api.ts && deno fmt src/generatedCode/pbd-core-api-services.ts",
    "gulp:copy:swagger": "gulp copySwagger"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "nswag": "^14.0.6",
    "gulp": "^4.0.2"
  }
}
